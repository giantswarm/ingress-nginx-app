{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "baseDomain": {
            "type": "string"
        },
        "cluster": {
            "type": "object",
            "properties": {
                "profile": {
                    "type": "integer"
                }
            }
        },
        "clusterID": {
            "type": "string"
        },
        "configmap": {
            "type": "object",
            "properties": {
                "error-log-level": {
                    "type": "string"
                },
                "hsts": {
                    "type": "string"
                },
                "server-name-hash-bucket-size": {
                    "type": "string"
                },
                "use-forwarded-headers": {
                    "type": "string"
                },
                "worker-processes": {
                    "type": "string"
                },
                "worker-shutdown-timeout": {
                    "type": "string"
                }
            }
        },
        "controller": {
            "type": "object",
            "properties": {
                "admissionWebhooks": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "failurePolicy": {
                            "type": "string",
                            "enum": ["Ignore", "Fail"]
                        },
                        "timeoutSeconds": {
                            "type": "integer",
                            "minimum": 0
                        },
                        "patch": {
                            "type": "object",
                            "properties": {
                                "backoffLimit": {
                                    "type": "integer"
                                },
                                "enabled": {
                                    "type": "boolean"
                                },
                                "image": {
                                    "type": "object",
                                    "properties": {
                                        "repository": {
                                            "type": "string"
                                        },
                                        "tag": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "nodeSelector": {
                                    "type": "object"
                                },
                                "podAnnotations": {
                                    "type": "object"
                                },
                                "priorityClassName": {
                                    "type": "string"
                                },
                                "runAsUser": {
                                    "type": "integer"
                                },
                                "tolerations": {
                                    "type": "array"
                                }
                            }
                        },
                        "port": {
                            "type": "integer"
                        },
                        "service": {
                            "type": "object",
                            "properties": {
                                "annotations": {
                                    "type": "object"
                                },
                                "servicePort": {
                                    "type": "integer"
                                },
                                "type": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "annotationsPrefix": {
                    "type": "string"
                },
                "antiAffinityScheduling": {
                    "type": "string"
                },
                "autoscaling": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "maxReplicas": {
                            "type": "integer"
                        },
                        "minReplicas": {
                            "type": "integer"
                        },
                        "targetCPUUtilizationPercentage": {
                            "type": "integer"
                        },
                        "targetMemoryUtilizationPercentage": {
                            "type": "integer"
                        }
                    }
                },
                "containerPort": {
                    "type": "object",
                    "properties": {
                        "http": {
                            "type": "integer"
                        },
                        "https": {
                            "type": "integer"
                        }
                    }
                },
                "defaultSSLCertificate": {
                    "type": "string"
                },
                "enableMimalloc": {
                    "type": "boolean"
                },
                "extraAnnotations": {
                    "type": "object",
                    "properties": {
                        "deployment": {
                            "type": "array"
                        },
                        "pod": {
                            "type": "array"
                        }
                    }
                },
                "extraEnvs": {
                    "type": "array"
                },
                "groupID": {
                    "type": "integer"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "ingressClass": {
                    "type": "string"
                },
                "lifecycle": {
                    "type": "object",
                    "properties": {
                        "preStop": {
                            "type": "object",
                            "properties": {
                                "exec": {
                                    "type": "object",
                                    "properties": {
                                        "command": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "livenessProbe": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "failureThreshold": {
                            "type": "integer"
                        },
                        "initialDelaySeconds": {
                            "type": "integer"
                        },
                        "periodSeconds": {
                            "type": "integer"
                        },
                        "successThreshold": {
                            "type": "integer"
                        },
                        "timeoutSeconds": {
                            "type": "integer"
                        }
                    }
                },
                "maxUnavailable": {
                    "type": "integer"
                },
                "metrics": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "integer"
                        },
                        "service": {
                            "type": "object",
                            "properties": {
                                "servicePort": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                },
                "minReadySeconds": {
                    "type": "integer"
                },
                "profile": {
                    "type": "object",
                    "properties": {
                        "small": {
                            "type": "object",
                            "properties": {
                                "resources": {
                                    "type": "object",
                                    "properties": {
                                        "requests": {
                                            "type": "object",
                                            "properties": {
                                                "cpu": {
                                                    "type": ["string", "integer"]
                                                },
                                                "memory": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "readinessProbe": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "failureThreshold": {
                            "type": "integer"
                        },
                        "initialDelaySeconds": {
                            "type": "integer"
                        },
                        "periodSeconds": {
                            "type": "integer"
                        },
                        "successThreshold": {
                            "type": "integer"
                        },
                        "timeoutSeconds": {
                            "type": "integer"
                        }
                    }
                },
                "replicaCount": {
                    "type": "integer"
                },
                "resources": {
                    "type": "object",
                    "properties": {
                        "requests": {
                            "type": "object",
                            "properties": {
                                "cpu": {
                                    "type": ["string", "integer"]
                                },
                                "memory": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "externalDNS": {
                            "type": "object",
                            "properties": {
                                "annotation": {
                                    "type": "string"
                                }
                            }
                        },
                        "externalTrafficPolicy": {
                            "type": "string"
                        },
                        "internal": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "externalTrafficPolicy": {
                                    "type": "string"
                                },
                                "nodePorts": {
                                    "type": "object",
                                    "properties": {
                                        "http": {
                                            "type": "integer"
                                        },
                                        "https": {
                                            "type": "integer"
                                        }
                                    }
                                },
                                "ports": {
                                    "type": "object",
                                    "properties": {
                                        "http": {
                                            "type": "integer"
                                        },
                                        "https": {
                                            "type": "integer"
                                        }
                                    }
                                },
                                "subdomain": {
                                    "type": "string"
                                },
                                "type": {
                                    "type": "string"
                                }
                            }
                        },
                        "nodePorts": {
                            "type": "object",
                            "properties": {
                                "http": {
                                    "type": "integer"
                                },
                                "https": {
                                    "type": "integer"
                                }
                            }
                        },
                        "ports": {
                            "type": "object",
                            "properties": {
                                "http": {
                                    "type": "integer"
                                },
                                "https": {
                                    "type": "integer"
                                }
                            }
                        },
                        "public": {
                            "type": "boolean"
                        },
                        "subdomain": {
                            "type": "string"
                        },
                        "targetPorts": {
                            "type": "object",
                            "properties": {
                                "http": {
                                    "type": "string"
                                },
                                "https": {
                                    "type": "string"
                                }
                            }
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                },
                "startupProbe": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "failureThreshold": {
                            "type": "integer"
                        },
                        "initialDelaySeconds": {
                            "type": "integer"
                        },
                        "periodSeconds": {
                            "type": "integer"
                        },
                        "successThreshold": {
                            "type": "integer"
                        },
                        "timeoutSeconds": {
                            "type": "integer"
                        }
                    }
                },
                "terminationGracePeriodSeconds": {
                    "type": "integer"
                },
                "updateIngressStatus": {
                    "type": "boolean"
                },
                "userID": {
                    "type": "integer"
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "registry": {
                    "type": "string"
                }
            }
        },
        "ingressController": {
            "type": "object",
            "properties": {
                "configmap": {
                    "type": "object"
                },
                "controlPlane": {
                    "type": "boolean"
                }
            }
        },
        "initContainer": {
            "type": "object",
            "properties": {
                "groupID": {
                    "type": "integer"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "name": {
                    "type": "string"
                },
                "userID": {
                    "type": "integer"
                }
            }
        },
        "podSecurityPolicy": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "provider": {
            "type": "string"
        },
        "test": {
            "type": "object",
            "properties": {
                "image": {
                    "type": "object",
                    "properties": {
                        "registry": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}
